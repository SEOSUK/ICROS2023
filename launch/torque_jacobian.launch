<launch>

<!--For DoB&Current based position control-->
  <arg name="position_p_gain"          default="400"/>
  <arg name="position_i_gain"          default="0"/>
  <arg name="position_d_gain"          default="400"/>
  <arg name="polar_moment_1"             default="0.0027"/>
  <arg name="polar_moment_2"             default="0.0027"/>
  <arg name="Cut_Off_Freq"             default="5"/>

  <arg name="virtual_mass_x"              default="1.0"/>
  <arg name="virtual_damper_x"            default="10.0"/>
  <arg name="virtual_spring_x"            default="0.1"/>

  <arg name="virtual_mass_y"              default="1.0"/>
  <arg name="virtual_damper_y"            default="0.1"/>
  <arg name="virtual_spring_y"            default="0.1"/>

  <arg name="cut_off_freq_current"       default="10"/>


<!--end-->


  <arg name="robot_name"      default="dasom"/>

  <arg name="Y_P_gain"          default="0"/>
  <arg name="Y_I_gain"          default="0"/>
  <arg name="Y_D_gain"          default="0"/>

  <arg name="VX_P_gain"         default="0"/>
  <arg name="VX_I_gain"         default="0"/>
  <arg name="VX_D_gain"         default="0"/>

  <arg name="VY_P_gain"         default="0"/>
  <arg name="VY_I_gain"         default="0"/>
  <arg name="VY_D_gain"         default="0"/>

  <arg name="Error_Gain"           default="3"/>
  <arg name="damping_const"        default="0.1"/>

  <arg name="stiction_alpha"       default="13.0"/>
  <arg name="stiction_k"           default="1.3"/>




  <param name="position_p_gain"        value="$(arg position_p_gain)"/>
  <param name="position_i_gain"        value="$(arg position_i_gain)"/>
  <param name="position_d_gain"        value="$(arg position_d_gain)"/>
  <param name="Cut_Off_Freq"           value="$(arg Cut_Off_Freq)"/>

  <param name="virtual_mass_x"           value="$(arg virtual_mass_x)"/>
  <param name="virtual_damper_x"           value="$(arg virtual_damper_x)"/>
  <param name="virtual_spring_x"           value="$(arg virtual_spring_x)"/>

  <param name="virtual_mass_y"           value="$(arg virtual_mass_y)"/>
  <param name="virtual_damper_y"           value="$(arg virtual_damper_y)"/>
  <param name="virtual_spring_y"           value="$(arg virtual_spring_y)"/>


  <param name="robot_name"    value="$(arg robot_name)"/>

  <param name="Y_P_gain"        value="$(arg Y_P_gain)"/>
  <param name="Y_I_gain"        value="$(arg Y_I_gain)"/>
  <param name="Y_D_gain"        value="$(arg Y_D_gain)"/>

  <param name="VX_P_gain"        value="$(arg VX_P_gain)"/>
  <param name="VX_I_gain"        value="$(arg VX_I_gain)"/>
  <param name="VX_D_gain"        value="$(arg VX_D_gain)"/>

  <param name="VY_P_gain"        value="$(arg VY_P_gain)"/>
  <param name="VY_I_gain"        value="$(arg VY_I_gain)"/>
  <param name="VY_D_gain"        value="$(arg VY_D_gain)"/>

  <param name="Cut_Off_Freq"        value="$(arg Cut_Off_Freq)"/>
  <param name="Error_Gain"        value="$(arg Error_Gain)"/>
  <param name="cut_off_freq_current"        value="$(arg cut_off_freq_current)"/>
  <param name="damping_const"        value="$(arg damping_const)"/>

  <param name="polar_moment_1"        value="$(arg polar_moment_1)"/>
  <param name="polar_moment_2"        value="$(arg polar_moment_2)"/>


  <param name="stiction_alpha"        value="$(arg stiction_alpha)"/>
  <param name="stiction_k"            value="$(arg stiction_k)"/>



  <node name="torque_jacobian" pkg="two_link" type="torque_jacobian" required="true" output="screen"/>

</launch>